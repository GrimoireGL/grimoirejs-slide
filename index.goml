    <text text="ハンズオン" font="Bold 35pt Noto Sans CJK JP" size="0.5" textAlign="center" position="0.4,3,0" color="#381794"></text>
    <mesh texture="./src/page/pictures/img4.jpg" scale="6"></mesh>
  </page>
  <page color="#f9efd5" transition="reveal" duration="2" easing="swifter">
    <camera></camera>
    <light></light>
    <text text="ハンズオン" font="Bold 35pt Noto Sans CJK JP" size="0.5" textAlign="center" position="0.4,3,0" color="#381794"></text>
    <mesh texture="./src/page/pictures/img5.jpg" scale="6"></mesh>
  </page>
  <page color="#f9efd5" transition="reveal" duration="2" easing="swifter">
    <camera></camera>
    <light></light>
    <text text="ハッカソン" font="Bold 35pt Noto Sans CJK JP" size="0.5" textAlign="center" position="0.4,3,0" color="#381794"></text>
    <mesh texture="./src/page/pictures/img6.png" scale="6"></mesh>
  </page>
  <page color="#f9efd5" transition="reveal" duration="2" easing="swifter">
