    <light></light>
    <text text="ハッカソン" font="Bold 35pt Noto Sans CJK JP" size="0.5" textAlign="center" position="0.4,3,0" color="#381794"></text>
    <mesh texture="./src/page/pictures/img7.png" scale="6"></mesh>
  </page>
  <page color="#f9efd5" transition="reveal" duration="2" easing="swifter">
    <camera></camera>
    <light></light>
    <text text="Global Game Jam" font="Bold 35pt Noto Sans CJK JP" size="0.5" textAlign="center" position="0.4,3,0" color="#381794"></text>
    <mesh texture="./src/page/pictures/img8.png" scale="6"></mesh>
  </page>
  <page color="#f9efd5" transition="reveal" duration="2" easing="swifter">
    <camera></camera>
    <light></light>
    <text text="Global Game Jam" font="Bold 35pt Noto Sans CJK JP" size="0.5" textAlign="center" position="0.4,3,0" color="#381794"></text>
    <mesh texture="./src/page/pictures/img9.jpg" scale="6"></mesh>
  </page>
  <page color="#f9efd5" transition="push" duration="1" easing="swifter">
    <camera></camera>
    <light></light>
    <text text="コミュニティの方の作品" font="Bold 35pt Noto Sans CJK JP" size="0.5" textAlign="center" position="0.4,3,0" color="#381794"></text>
  </page>
  <page color="#f9efd5" transition="push" duration="1" easing="swifter">
    <camera></camera>
    <light></light>
    <mesh texture="./src/page/pictures/future-01.png" position="2,0,3" scale="2" material="new(unlit)"></mesh>
    <mesh texture="./src/page/pictures/future-02.png" position="-2,0,3" scale="2" material="new(unlit)"></mesh>
    <mesh material="new(futureShader)" class="future-shader"/>  </page>
  <page color="black" transition="push" duration="1" easing="swifter">
    <camera></camera>
    <light></light>
    <text text="END" font="Bold 35pt Noto Sans CJK JP" size="0.5" textAlign="center" position="0.4,0,0" color="#f9efd5"></text>
  </page>
