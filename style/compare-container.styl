.compare-container
  position absolute
  left 0
  top 0
  height 100%
  width 50%

  .caption
    position absolute
    text-align center
    font-size 5vh
    text-shadow 0 3px 10px rgba(0, 0, 0, 0.7)
    width 100%
    top 2%
    color #381794
    font-weight bold
    z-index 4

  .flex
    height 100%
    width 100%
    display flex
    justify-content center
    align-items center

  .editor
    margin 0 20px 20px 20px
    height calc(100% - 300px - 20px)
    width calc(100% - 40px)

